#: src/guess.c:13
msgid "Think of a number between 1 and 100.\n"
msgstr "Загадайте число от 1 до 100.\n"

#: src/guess.c:21
#, c-format
msgid "Is the number greater than %d? (yes/no)\n"
msgstr "Число больше %d? (да/нет)\n"

#: src/guess.c:26
msgid "Error: Unexpected EOF or input error.\n"
msgstr "Ошибка: Неожиданный конец файла или ошибка ввода.\n"

#: src/guess.c:30
msgid "yes"
msgstr "да"

#: src/guess.c:32
msgid "no"
msgstr "нет"

#: src/guess.c:35
msgid "Invalid input. Please answer 'yes' or 'no'.\n"
msgstr "Неверный ввод. Пожалуйста, ответьте 'да' или 'нет'.\n"

#: src/guess.c:40
#, c-format
msgid "Your number is %d!\n"
msgstr "Ваше число %d!\n"
